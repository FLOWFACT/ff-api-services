{
  "name": "@flowfact/api-services",
  "version": "15.0.0-SNAPSHOT",
  "description": "middleware of flowfact to interact between frontend and backend",
  "main": "build/index.js",
  "module": "build/es/index.js",
  "types": "build/index.d.ts",
  "repository": "ssh://git@github.com:FLOWFACT/ff-api-services.git",
  "author": "FLOWFACT GmbH",
  "license": "SEE LICENSE IN LICENSE",
  "sideEffects": false,
  "scripts": {
    "build:cjs": "tsc",
    "build:esm": "tsc --project tsconfig.esm.json",
    "prepare": "rimraf build/ && yarn run build:cjs && yarn run build:esm",
    "lint": "eslint src/*",
    "doc": "typedoc --out doc/",
    "local": "node pack.js platform-apps",
    "upgradeTypes": "yarn && yarn add @flowfact/types --latest",
    "upgradeNodeFlowdsl": "yarn add @flowfact/node-flowdsl --latest"
  },
  "dependencies": {
    "@flowfact/node-flowdsl": "^2.20200707.32",
    "@flowfact/types": "^3.20200608.149",
    "@types/graphql": "14.0.7",
    "@types/node": "^11.15.4",
    "@types/prop-types": "^15.5.1",
    "@types/qs": "^6.5.1",
    "@types/storejs": "^1.3.1",
    "@types/uuid": "^3.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "aws-amplify": "1.1.19",
    "axios": "^0.19.2",
    "detect-node": "^2.0.3",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "qs": "^6.5.2",
    "store": "^2.0.12",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "amazon-cognito-identity-js": "3.0.9",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "recursive-search": "^1.0.1",
    "rimraf": "^2.7.1",
    "typescript": "^3.7.4"
  }
}
